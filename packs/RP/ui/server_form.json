{
	"namespace": "server_form",
	"custom_form": {
		"$protocol_header": "bcuiv0001",
		"modifications": [
			{
				"array_name": "bindings",
				"operation": "insert_back",
				"value": [
					{
						"binding_name": "#title_text"
					},
					{
						"binding_type": "view",
						"source_property_name": "((#title_text - $protocol_header) = #title_text)",
						"target_property_name": "#visible"
					}
				]
			}
		]
	},
	"main_screen_content": {
		"modifications": [
			{
				"array_name": "controls",
				"operation": "insert_back",
				"value": [
					{
						"core-ui_server_form_factory": {
							"type": "panel",
							"factory": {
								"name": "server_form_factory",
								"control_ids": {
									"custom_form": "@core-ui_common.screen_container"
								}
							}
						}
					}
				]
			}
		]
	}
}