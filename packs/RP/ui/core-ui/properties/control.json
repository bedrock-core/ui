{
  "namespace": "core-ui_properties",
  "control": {
    "visible": "#visible",
    "enabled": "#enabled",
    "layer": "#layer",
    "bindings": [
      {
        "binding_name": "#custom_text",
        "binding_type": "collection",
        "binding_collection_name": "custom_form"
      },
      /* --- strip protocol header (9 chars) --- */
      {
        "binding_type": "view",
        "source_property_name": "('%.9s' * #custom_text)",
        "target_property_name": "#protocol_header"
      },
      {
        "binding_type": "view",
        "source_property_name": "(#custom_text - #protocol_header)",
        "target_property_name": "#rem_after_header"
      },
      /* --- type (string) - field 0 --- */
      {
        "binding_type": "view",
        "source_property_name": "('%.35s' * #rem_after_header)",
        "target_property_name": "#raw_type"
      },
      {
        "binding_type": "view",
        "source_property_name": "(#rem_after_header - #raw_type)",
        "target_property_name": "#rem_after_type"
      },
      {
        "binding_type": "view",
        "source_property_name": "('%.34s' * #raw_type)",
        "target_property_name": "#raw_type_core"
      },
      {
        "binding_type": "view",
        "source_property_name": "('%.32s' * #raw_type_core)",
        "target_property_name": "#type_padded"
      },
      {
        "binding_type": "view",
        "source_property_name": "('%.2s' * #raw_type_core)",
        "target_property_name": "#type_prefix"
      },
      {
        "binding_type": "view",
        "source_property_name": "((#type_padded - #type_prefix) - ';')",
        "target_property_name": "#type"
      },
      /* --- visible (bool) - field 1 --- */
      {
        "binding_type": "view",
        "source_property_name": "('%.8s' * #rem_after_type)",
        "target_property_name": "#raw_visible"
      },
      {
        "binding_type": "view",
        "source_property_name": "(#rem_after_type - #raw_visible)",
        "target_property_name": "#rem_after_visible"
      },
      {
        "binding_type": "view",
        "source_property_name": "('%.7s' * #raw_visible)",
        "target_property_name": "#raw_visible_core"
      },
      {
        "binding_type": "view",
        "source_property_name": "('%.5s' * #raw_visible_core)",
        "target_property_name": "#visible_padded"
      },
      {
        "binding_type": "view",
        "source_property_name": "('%.2s' * #raw_visible_core)",
        "target_property_name": "#visible_prefix"
      },
      {
        "binding_type": "view",
        "source_property_name": "((#visible_padded - #visible_prefix) - ';')",
        "target_property_name": "#visible"
      },
      /* --- enabled (bool) - field 2 --- */
      {
        "binding_type": "view",
        "source_property_name": "('%.8s' * #rem_after_visible)",
        "target_property_name": "#raw_enabled"
      },
      {
        "binding_type": "view",
        "source_property_name": "(#rem_after_visible - #raw_enabled)",
        "target_property_name": "#rem_after_enabled"
      },
      {
        "binding_type": "view",
        "source_property_name": "('%.7s' * #raw_enabled)",
        "target_property_name": "#raw_enabled_core"
      },
      {
        "binding_type": "view",
        "source_property_name": "('%.5s' * #raw_enabled_core)",
        "target_property_name": "#enabled_padded"
      },
      {
        "binding_type": "view",
        "source_property_name": "('%.2s' * #raw_enabled_core)",
        "target_property_name": "#enabled_prefix"
      },
      {
        "binding_type": "view",
        "source_property_name": "((#enabled_padded - #enabled_prefix) - ';')",
        "target_property_name": "#enabled"
      },
      /* --- layer (int) - field 3 --- */
      {
        "binding_type": "view",
        "source_property_name": "('%.19s' * #rem_after_enabled)",
        "target_property_name": "#raw_layer"
      },
      {
        "binding_type": "view",
        "source_property_name": "(#rem_after_enabled - #raw_layer)",
        "target_property_name": "#rem_after_layer"
      },
      {
        "binding_type": "view",
        "source_property_name": "('%.18s' * #raw_layer)",
        "target_property_name": "#raw_layer_core"
      },
      {
        "binding_type": "view",
        "source_property_name": "('%.16s' * #raw_layer_core)",
        "target_property_name": "#layer_padded"
      },
      {
        "binding_type": "view",
        "source_property_name": "('%.2s' * #raw_layer_core)",
        "target_property_name": "#layer_prefix"
      },
      {
        "binding_type": "view",
        "source_property_name": "((#layer_padded - #layer_prefix) - ';')",
        "target_property_name": "#layer"
      },
      /* --- skip __controlReserved (reserved) - field 4 - 96 bytes total --- */
      {
        "binding_type": "view",
        "source_property_name": "('%.96s' * #rem_after_layer)",
        "target_property_name": "#skip_control_reserved"
      },
      {
        "binding_type": "view",
        "source_property_name": "(#rem_after_layer - #skip_control_reserved)",
        "target_property_name": "#rem_after_control_reserved"
      }
    ]
  }
}