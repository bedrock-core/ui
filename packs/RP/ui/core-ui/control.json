{
	"namespace": "core-ui_properties",
	"control": {
		"type": "panel",
		"size": [
			"100%c",
			"100%c"
		],
		"controls": [
			{
				"root@common.common_panel": {
					"type": "panel",
					// Required to be able to use size_binding_x/y
					// https://discord.com/channels/523663022053392405/1067870274894172260/1369710838197391430
					// https://discord.com/channels/523663022053392405/1067870274894172260/1363931489636450435
					// https://discord.com/channels/523663022053392405/1067870274894172260/1363895356764717157
					// https://discord.com/channels/523663022053392405/1347664226931703839/1347667681205288990
					// https://discord.com/channels/523663022053392405/1067870274894172260/1341531305162047518
					"visible": "#visible", // working
					"enabled": "#enabled", // TODO PENDING TESTING SOMEHOW
					"layer": "#layer", // TODO PENDING TESTING SOMEHOW
					// Fucking sizing and Dangling number (no % or px in Size)
					// for some reason in json ui if a binding has as first character
					// a digit it breaks completely and looses any "text" behind the digits
					// also could be other issue idk
					"size": [
						"100%",
						"12px"
					],
					"anchor_from": "bottom_middle",
					"anchor_to": "bottom_middle",
					"use_anchored_offset": true,
					"property_bag": {
						"#size_binding_x": 0.0,
						"#size_binding_y": 0.0
					},
					// "offset": [
					// 	"#x",
					// 	"#y"
					// ],
					"inherit_max_sibling_width": "#inherit_max_sibling_width",
					"inherit_max_sibling_height": "#inherit_max_sibling_height",
					"bindings": [
						{
							"binding_name": "#custom_text",
							"binding_type": "collection",
							"binding_collection_name": "custom_form"
						},
						/* --- strip protocol header (9 chars) --- */
						{
							"binding_type": "view", // protocol header size
							"source_property_name": "('%.9s' * #custom_text)",
							"target_property_name": "#protocol_header"
						},
						{
							"binding_type": "view",
							"source_property_name": "(#custom_text - #protocol_header)",
							"target_property_name": "#rem_after_header"
						},
						// /* --- type (string) - field 0 --- */
						{
							"binding_type": "view", // full_width
							"source_property_name": "('%.35s' * #rem_after_header)",
							"target_property_name": "#raw_type"
						},
						{
							"binding_type": "view",
							"source_property_name": "(#rem_after_header - #raw_type)",
							"target_property_name": "#rem_after_type"
						},
						{
							"binding_type": "view", // (full_width - marker_width) - prefix_width - padding_char (;)
							"source_property_name": "(('%.34s' * #raw_type) - ('%.2s' * #raw_type) - ';')",
							"target_property_name": "#type"
						},
						/* --- width (string) - field 1 --- */
						{
							"binding_type": "view", // full_width
							"source_property_name": "('%.35s' * #rem_after_type)",
							"target_property_name": "#raw_width"
						},
						{
							"binding_type": "view",
							"source_property_name": "(#rem_after_type - #raw_width)",
							"target_property_name": "#rem_after_width"
						},
						{
							"binding_type": "view", // (full_width - marker_width) - prefix_width - padding_char (;)
							"source_property_name": "(('%.34s' * #raw_width) - ('%.2s' * #raw_width) - ';')",
							"target_property_name": "#width"
						},
						{
							"binding_type": "view",
							"source_property_name": "#width",
							"target_property_name": "#size_binding_x"
						},
						/* --- height (string) - field 2 --- */
						{
							"binding_type": "view", // full_width
							"source_property_name": "('%.35s' * #rem_after_width)",
							"target_property_name": "#raw_height"
						},
						{
							"binding_type": "view",
							"source_property_name": "(#rem_after_width - #raw_height)",
							"target_property_name": "#rem_after_height"
						},
						{
							"binding_type": "view", // (full_width - marker_width) - prefix_width - padding_char (;)
							"source_property_name": "(('%.34s' * #raw_height) - ('%.2s' * #raw_height) - ';')",
							"target_property_name": "#height"
						},
						{
							"binding_type": "view",
							"source_property_name": "#height",
							"target_property_name": "#size_binding_y"
						},
						/* --- x (string) - field 3 --- */
						{
							"binding_type": "view", // full_width
							"source_property_name": "('%.35s' * #rem_after_height)",
							"target_property_name": "#raw_x"
						},
						{
							"binding_type": "view",
							"source_property_name": "(#rem_after_height - #raw_x)",
							"target_property_name": "#rem_after_x"
						},
						{
							"binding_type": "view", // (full_width - marker_width) - prefix_width - padding_char (;)
							"source_property_name": "(('%.34s' * #raw_x) - ('%.2s' * #raw_x) - ';')",
							"target_property_name": "#x"
						},
						/* --- y (string) - field 4 --- */
						{
							"binding_type": "view", // full_width
							"source_property_name": "('%.35s' * #rem_after_x)",
							"target_property_name": "#raw_y"
						},
						{
							"binding_type": "view",
							"source_property_name": "(#rem_after_x - #raw_y)",
							"target_property_name": "#rem_after_y"
						},
						{
							"binding_type": "view", // (full_width - marker_width) - prefix_width - padding_char (;)
							"source_property_name": "(('%.34s' * #raw_y) - ('%.2s' * #raw_y) - ';')",
							"target_property_name": "#y"
						},
						/* --- visible (bool) - field 5 --- */
						{
							"binding_type": "view", // full_width
							"source_property_name": "('%.8s' * #rem_after_y)",
							"target_property_name": "#raw_visible"
						},
						{
							"binding_type": "view",
							"source_property_name": "(#rem_after_y - #raw_visible)",
							"target_property_name": "#rem_after_visible"
						},
						{
							"binding_type": "view", // (full_width - marker_width) - prefix_width (s:) - padding_char (;)
							"source_property_name": "(('%.7s' * #raw_visible) - ('%.2s' * #raw_visible) - ';')",
							"target_property_name": "#visible"
						},
						/* --- enabled (bool) - field 6 --- */
						{
							"binding_type": "view", // full_width
							"source_property_name": "('%.8s' * #rem_after_visible)",
							"target_property_name": "#raw_enabled"
						},
						{
							"binding_type": "view",
							"source_property_name": "(#rem_after_visible - #raw_enabled)",
							"target_property_name": "#rem_after_enabled"
						},
						{
							"binding_type": "view", // (full_width - marker_width) - prefix_width - padding_char (;)
							"source_property_name": "(('%.7s' * #raw_enabled) - ('%.2s' * #raw_enabled) - ';')",
							"target_property_name": "#enabled"
						},
						/* --- layer (int) - field 7 --- */
						{
							"binding_type": "view", // full_width
							"source_property_name": "('%.19s' * #rem_after_enabled)",
							"target_property_name": "#raw_layer"
						},
						{
							"binding_type": "view",
							"source_property_name": "(#rem_after_enabled - #raw_layer)",
							"target_property_name": "#rem_after_layer"
						},
						{
							"binding_type": "view", // (full_width - marker_width) - prefix_width - padding_char (;)
							"source_property_name": "(('%.18s' * #raw_layer) - ('%.2s' * #raw_layer) - ';')",
							"target_property_name": "#layer"
						},
						/* --- inherit_max_sibling_width (bool) - field 8 --- */
						{
							"binding_type": "view", // full_width
							"source_property_name": "('%.8s' * #rem_after_layer)",
							"target_property_name": "#raw_inherit_max_sibling_width"
						},
						{
							"binding_type": "view",
							"source_property_name": "(#rem_after_layer - #raw_inherit_max_sibling_width)",
							"target_property_name": "#rem_after_inherit_max_sibling_width"
						},
						{
							"binding_type": "view", // (full_width - marker_width) - prefix_width - padding_char (;)
							"source_property_name": "(('%.7s' * #raw_inherit_max_sibling_width) - ('%.2s' * #raw_inherit_max_sibling_width) - ';')",
							"target_property_name": "#inherit_max_sibling_width"
						},
						/* --- inherit_max_sibling_height (bool) - field 9 --- */
						{
							"binding_type": "view", // full_width
							"source_property_name": "('%.8s' * #rem_after_inherit_max_sibling_width)",
							"target_property_name": "#raw_inherit_max_sibling_height"
						},
						{
							"binding_type": "view",
							"source_property_name": "(#rem_after_inherit_max_sibling_width - #raw_inherit_max_sibling_height)",
							"target_property_name": "#rem_after_inherit_max_sibling_height"
						},
						{
							"binding_type": "view", // (full_width - marker_width) - prefix_width - padding_char (;)
							"source_property_name": "(('%.7s' * #raw_inherit_max_sibling_height) - ('%.2s' * #raw_inherit_max_sibling_height) - ';')",
							"target_property_name": "#inherit_max_sibling_height"
						},
						/* --- skip __reserved --- */
						{
							"binding_type": "view", // reserved full_width
							"source_property_name": "('%.277s' * #rem_after_inherit_max_sibling_height)",
							"target_property_name": "#skip_reserved_control"
						},
						{
							"binding_type": "view",
							"source_property_name": "(#rem_after_inherit_max_sibling_height - #skip_reserved_control)",
							"target_property_name": "#serialized_data"
						}
					]
				}
			}
		]
	},
	"simple_control": {
		"type": "panel",
		"size": [
			"100%c",
			"100%c"
		],
		"controls": [
			{
				"value_2@common.common_panel": {
					"type": "label",
					"text": "#custom_text",
					"size": [
						"100%",
						"20px"
					],
					"use_anchored_offset": true,
					"property_bag": {
						"#size_binding_x": 0.0,
						"#size_binding_y": 0.0
					},
					"bindings": [
						{
							"binding_name": "#custom_text",
							"binding_type": "collection",
							"binding_collection_name": "custom_form"
						},
						/* --- strip protocol header (44 chars) --- */
						{
							"binding_type": "view", // protocol header size
							"source_property_name": "('%.44s' * #custom_text)",
							"target_property_name": "#protocol_header"
						},
						{
							"binding_type": "view",
							"source_property_name": "(#custom_text - #protocol_header)",
							"target_property_name": "#rem_after_header"
						},
						/* --- width --- */
						{
							"binding_type": "view", // byte width
							"source_property_name": "('%.35s' * #rem_after_header)",
							"target_property_name": "#raw_width"
						},
						{
							"binding_type": "view",
							"source_property_name": "(#rem_after_header - #raw_width)",
							"target_property_name": "#rem_after_width"
						},
						{
							"binding_type": "view",
							// get 34 bytes (remove last char); from those 34 bytes remove first 2 bytes; remove padding (';')
							"source_property_name": "(('%.34s' * #raw_width) - ('%.2s' * #raw_width) - ';')",
							"target_property_name": "#width"
						},
						{
							"binding_type": "view",
							"source_property_name": "#width",
							"target_property_name": "#size_binding_x"
						},
						/* --- height --- */
						{
							"binding_type": "view", // byte width
							"source_property_name": "('%.35s' * #rem_after_width)",
							"target_property_name": "#raw_height"
						},
						{
							"binding_type": "view",
							"source_property_name": "(#rem_after_width - #raw_height)",
							"target_property_name": "#rem_after_height"
						},
						{
							"binding_type": "view",
							// get 34 bytes (remove last char); from those 34 bytes remove first 2 bytes; remove padding (';')
							"source_property_name": "(('%.34s' * #raw_height) - ('%.2s' * #raw_height) - ';')",
							"target_property_name": "#height"
						},
						{
							"binding_type": "view",
							"source_property_name": "#height",
							"target_property_name": "#size_binding_y"
						}
					]
				}
			}
		]
	}
}