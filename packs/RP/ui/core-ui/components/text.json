{
	"namespace": "core-ui_components",
	"text": {
		"type": "panel",
		"size": [
			10,
			10
		],
		"anchor_from": "center",
		"anchor_to": "center",
		"controls": [
			{
				"dynamic_ui_main": {
					"type": "image",
					"texture": "textures/ui/core-ui/text/background",
					"size": [
						20,
						50
					],
					"property_bag": {
						"#anchored_offset_value_y": 0.0,
						"#size_binding_x": 1.0,
						"#size_binding_y": 1.0
					},
					"bindings": [
						{
							"binding_name": "#title_text",
							"binding_type": "global"
						},
						{
							"binding_type": "view",
							"source_property_name": "(#title_text - 'dynamic:')",
							"target_property_name": "#text"
						},
						{
							"binding_type": "view",
							"source_property_name": "(('%.4s' * #text - ':') / 10)",
							"target_property_name": "#size_binding_x"
						},
						{
							"binding_type": "view",
							"source_property_name": "((#text - '%.5s' * #text) / 10)",
							"target_property_name": "#size_binding_y"
						}
					]
				}
			}
		]
	},
	"text2": {
		"type": "panel"
	}
}